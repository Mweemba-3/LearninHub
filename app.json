{
  "expo": {
    "name": "LearningHub",
    "slug": "LearningHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions":
      ["MEDIA_LIBRARY",
        "DOCUMENT_LIBRARY"
    ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.LearningHub"
    },
    "web": {
      "favicon": "./assets/images/icon.jpg"
    }
  }
}
